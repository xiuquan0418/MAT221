Cell[TextData[{
 "This is because ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"1", "-", "x"}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["1", "3"], "-", 
      SuperscriptBox[
       RowBox[{"(", 
        RadicalBox["x", "3"], ")"}], "3"]}], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RadicalBox["x", "3"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["x", 
         RowBox[{"2", "/", "3"}]], "+", 
        SuperscriptBox["x", 
         RowBox[{"1", "/", "3"}]], "+", "1"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"06047239-1ecb-4638-b39a-d9285bd701df"],
 "."
}], "Text",ExpressionUUID->"85a58083-e587-4dbc-9e6b-de4dfadb5c0c"],

