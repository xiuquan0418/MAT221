 RowBox[{"1", "+", 
  SuperscriptBox["x", 
   RowBox[{"1", "/", "3"}]], "+", 
  SuperscriptBox["x", 
   RowBox[{"2", "/", "3"}]]}]], "Output",ExpressionUUID->"531f969a-edd8-4abd-\
9aed-e315c0a62fe3"]
